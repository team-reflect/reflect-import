{
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "prepare": "tsup",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "reflect-import",
  "author": "Alex MacCaw",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "tsup": {
    "entry": ["src/index.ts"],
    "format": ["esm"],
    "dts": true,
    "clean": true
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.189",
    "husky": "^8.0.2",
    "size-limit": "^8.1.0",
    "tsup": "^6.5.0",
    "typescript": "^4.8.4",
    "vitest": "^0.25.2"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "js-yaml": "^4.1.0",
    "jsdom": "^20.0.3",
    "lodash": "^4.17.21",
    "mdast-util-gfm-autolink-literal": "^1.0.2",
    "micromark-extension-gfm-autolink-literal": "^1.0.3",
    "rehype-stringify": "^9.0.3",
    "remark-frontmatter": "^4.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-wiki-link": "^1.0.4",
    "unified": "^10.1.2"
  }
}
