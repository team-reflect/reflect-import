{
  "name": "@team-reflect/reflect-import",
  "type": "module",
  "version": "0.2.8",
  "author": "Alex MacCaw",
  "license": "MIT",
  "repository": "git://github.com/team-reflect/reflect-import.git",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "TZ=UTC vitest",
    "prepare": "tsup",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "lint": "eslint ."
  },
  "typings": "dist/index.d.ts",
  "dependencies": {
    "date-fns": "^2.29.3",
    "jsdom": "^20.0.3",
    "lodash": "^4.17.21",
    "mdast-util-gfm-autolink-literal": "^1.0.2",
    "mdast-util-to-string": "^3.1.0",
    "micromark-extension-gfm-autolink-literal": "^1.0.3",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-wiki-link": "^1.0.4",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1",
    "urlpattern-polyfill": "^6.0.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@ocavue/eslint-config": "^0.4.5",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.189",
    "eslint": "^8.28.0",
    "prettier": "^2.8.0",
    "size-limit": "^8.1.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vitest": "^0.25.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "100 KB"
    }
  ],
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "clean": true
  }
}
