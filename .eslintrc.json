{
  "extends": ["@ocavue/eslint-config"],

  "rules": {
    "import/order": [
      "warn",
      {
        "newlines-between": "always",
        "alphabetize": {"order": "asc"},
        "groups": [
          "builtin",
          "external",
          "internal",
          ["parent", "sibling"],
          "index",
          "type"
        ]
      }
    ]
  }
}
